<h1>Edit Picture</h1>

<%= form_for @picture do |f| %>
  <p> 
    <%= f.label "artist" %><br>
    <%= f.text_field "artist" %>
  </p>
  <p>
    <%= f.label "title" %><br>
    <%= f.text_field "title" %>
  </p>
  <p>
    <%= f.label "url" %><br>
    <%= f.text_field "url" %>
  </p>
  <p>
    <%= f.submit "Save Changes" %>
  </p>
<%= link_to "Back to Pictures", "/pictures" %>   <!-- link_to replaces a href; second parameter is URL, same as /pictures  -->
<% end %>
